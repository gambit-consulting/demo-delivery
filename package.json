{
    "name": "demo-delivery",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "https://github.com/gambit-consulting/demo-delivery",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "@sap/cds": "^7",
        "express": "^4"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/ux-specification": "^1.120.4",
        "eslint": "^8.57.0",
        "prettier": "^3.2.5"
    },
    "scripts": {
        "dev": "cds-ts watch",
        "format": "prettier --write --ignore-unknown .",
        "start": "cds-serve",
        "watch-manage-products": "cds watch --open manage-products/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-manage-orders": "cds watch --open manage-orders/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "sapux": [
        "app/manage-products",
        "app/manage-orders"
